a = float(input("Enter the English mark"))
b = float(input("Enter the tamil mark"))
c = float(input("Enter the Maths mark"))
d = float(input("Enter the science mark"))
e = float(input("Enter the social mark"))
PR = (a+b+c+d+e)/100
TO = (a+b+c+d+e)
AV = (TO/500)*100
print ("The Average is :" +str(AV))
print ("The Total is :" +str(TO))
print ("The Percentage is :" +str(PR)
